				            "default": "20140717",
				            "type":"string",
				            "description":"version"
				        }
				    }
				},
				"userCheckins": {
					"name": "User Checkins",
					"description": "Returns a history of checkins for the authenticated user.",
					"httpMethod": "GET",
					"path": "/users/{USER_ID}/checkins",
				    "parameters": {
				        "USER_ID": {
				            "title":"USER_ID", 
				            "required": true,
				            "default": "self",
				            "type":"integer",
				            "description":"A foursquare user id (e.g. 1297336)",
				            "location": "path"
				        },
				        "version": {
				            "title":"v", 
				            "required": true,
				            "default": "20140717",
				            "type":"string",
				            "description":"version"
				        },
		                "limit": {
		                    "title":"limit",
		                    "required": false,
		                    "default":"100",
		                    "type":"integer",
		                    "description":"Number of results to return, up to 250."
		                },
		                "offset": {
		                    "title":"offset",
		                    "required": false,
		                    "default":"100",
		                    "type":"integer",
		                    "description":"The number of results to skip. Used to page through results."
		                },
		                "sort": {
		                    "title":"sort",
		                    "required": false,
		                    "default":"newestfirst",
		                    "type":"string",
		                    "description":"How to sort the returned checkins. Can be newestfirst or oldestfirst."
		                },
		                "afterTimestamp": {
		                    "title":"afterTimestamp",
		                    "required": false,
		                    "default":"1279044824",
		                    "type":"integer",
		                    "description":"Retrieve the first results to follow these seconds since epoch. This should be useful for paging forward in time, or when polling for changes. To avoid missing results when polling, we recommend subtracting several seconds from the last poll time and then de-duplicating."
		                },
		                "beforeTimestamp": {
		                    "title":"beforeTimestamp",
		                    "required": false,
		                    "default":"1279044824",
		                    "type":"integer",
		                    "description":"Retrieve the first results prior to these seconds since epoch. Useful for paging backward in time."
		                }
				    }
				},
				"listFriends": {
					"name": "List Friends",
					"description": "Returns an array of a user's friends.",
					"httpMethod": "GET",
					"path": "/users/{USER_ID}/friends",
				    "parameters": {
				        "USER_ID": {
				            "title":"USER_ID", 
				            "required": true,
				            "default": "1297336",
				            "type":"integer",
				            "description":"A foursquare user id (e.g. 1297336)",
				            "location": "path"
				        },
				        "version": {
				            "title":"v", 
				            "required": true,
				            "default": "20140717",
				            "type":"string",
				            "description":"version"
				        },
				        "limit": {
				            "title":"limit", 
				            "required": false,
				            "default": "100",
				            "type":"integer",
				            "description":"Number of results to return, up to 500."
				        },
		                "offset": {
		                    "title":"offset",
		                    "required": false,
		                    "default":"100",
		                    "type":"integer",
		                    "description":"Used to page through results."
		                }
				    }
				},
				"mayorships": {
					"name": "Mayorships",
					"description": "A user's mayorships.",
					"httpMethod": "GET",
					"path": "/users/{USER_ID}/mayorships",
					"parameters": {
						"USER_ID": {
		                    "title":"USER_ID",
		                    "required": true,
		                    "default":"1297336",
		                    "type":"integer",
		                    "description":"A foursquare user id (e.g. 1297336)."
		                },
				        "version": {
				            "title":"v", 
				            "required": true,
				            "default": "20140717",
				            "type":"string",
				            "description":"version"
				        }
		            }
				},
				"photos": {
					"name": "photos",
					"description": "Returns photos from a user.",
					"httpMethod": "GET",
					"path": "/users/{USER_ID}/photos",
					"parameters": {
						"USER_ID": {
		                    "title":"USER_ID",
		                    "required": true,
		                    "default":"self",
		                    "type":"integer",
		                    "description":"For now, only self is supported."
		                },
				        "version": {
				            "title":"v", 
				            "required": true,
				            "default": "20140717",
				            "type":"string",
				            "description":"version"
				        },
		                "limit": {
		                    "title":"USER_ID",
		                    "required": false,
		                    "default":"100",
		                    "type":"integer",
		                    "description":"Number of results to return, up to 500."
		                },
						"offset": {
		                    "title":"offset",
		                    "required": false,
		                    "default":"100",
		                    "type":"integer",
		                    "description":"Used to page through results."
		                }
		            }
				},
				"venueHistory": {
					"name": "Venue History",
					"description": "Returns a list of all venues visited by the specified user, along with how many visits and when they were last there.",
					"httpMethod": "GET",
					"path": "/users/{USER_ID}/venuehistory",
					"parameters": {
						"USER_ID": {
		                    "title":"USER_ID",
		                    "required": true,
		                    "default":"self",
		                    "type":"integer",
		                    "description":"For now, only self is supported."
		                },
				        "version": {
				            "title":"v", 
				            "required": true,
				            "default": "20140717",
				            "type":"string",
				            "description":"version"
				        },
		                "beforeTimestamp": {
	                     	"title":"beforeTimestamp",
	                    	"required": false,
	                     	"default":"123456789",
	                     	"type":"integer",
	                     	"description":"Seconds since epoch."
		                },
		                  "afterTimestamp": {
		                    "title":"afterTimestamp",
		                    "required": false,
		                    "default":"123456789",
		                    "type":"integer",
		                    "description":"Seconds since epoch."
		                },
		                  "categoryId": {
		                    "title":"categoryId",
		                    "required":false,
		                    "default":"asad13242ljla",
		                    "type":"string",
		                    "description":"Limits returned venues to those in this category. If specifying a top-level category, all sub-categories will also match the query."
		                }
		            }
		        }
			}
		},
		"Users: Action Methods": {
